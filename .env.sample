# Environment sample for Project Chimera

# Model/API keys (replace with your keys)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenClaw (local or remote)
OPENCLAW_API_URL=http://127.0.0.1:9000
OPENCLAW_API_KEY=your_openclaw_api_key_here

# Tenx MCP Sense (Telemetry)
TENX_MCP_SENSE_ENDPOINT=https://mcppulse.10academy.org/proxy
